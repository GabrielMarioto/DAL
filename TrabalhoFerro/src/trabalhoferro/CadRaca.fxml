<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<SplitPane dividerPositions="" focusTraversable="true" maxHeight="360.0" maxWidth="700.0" minHeight="360.0" minWidth="700.0" prefHeight="360.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="trabalhoferro.CadRacaController">
  <items>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="516.0" style="-fx-background-color: orange">
          <children>
            <Button fx:id="btn_Novo" mnemonicParsing="false" onAction="#evt_Novo" text="Novo">
              <font>
                <Font name="Comic Sans MS" size="12.0" fx:id="x1" />
              </font>
              <HBox.margin>
                <Insets right="10.0" fx:id="x2" />
              </HBox.margin>
            </Button>
            <Button fx:id="btn_Alterar" font="$x1" mnemonicParsing="false" onAction="#evt_Alterar" text="Alterar" HBox.margin="$x2" />
            <Button id="evt_Apagar" fx:id="btn_Apagar" font="$x1" mnemonicParsing="false" onAction="#evt_Apagar" text="Apagar" HBox.margin="$x2" />
            <Button fx:id="btn_Confirmar" font="$x1" mnemonicParsing="false" onAction="#evt_Confirmar" text="Confirmar" HBox.margin="$x2" />
            <Button fx:id="btn_Cancelar" font="$x1" mnemonicParsing="false" onAction="#evt_Cancelar" text="Cancelar" HBox.margin="$x2" />
          </children>
        </HBox>
        <AnchorPane fx:id="pnl_Cadastro" prefHeight="64.0" prefWidth="516.0" style="-fx-background-color:  #4F4F4F;">
          <children>
            <Label layoutX="14.0" layoutY="14.0" text="Código da Raça:">
              <textFill>
                <Color blue="0.000" green="0.720" red="1.000" fx:id="x3" />
              </textFill>
            </Label>
            <Label layoutX="149.0" layoutY="17.0" text="Nome da Raça:" textFill="$x3" />
            <TextField fx:id="tb_codigo" layoutX="14.0" layoutY="37.0" prefWidth="103.0" />
            <TextField fx:id="tb_nome" layoutX="149.0" layoutY="37.0" prefWidth="200.0" />
          </children>
        </AnchorPane>
        <FlowPane alignment="CENTER" prefHeight="35.0" prefWidth="516.0" style="-fx-background-color: orange">
          <children>
            <TextField fx:id="tb_pesquisa" prefWidth="200.0" />
            <Button fx:id="btn_Pesquisar" font="$x1" mnemonicParsing="false" onAction="#evt_Pesquisar" text="Pesquisar">
              <FlowPane.margin>
                <Insets left="10.0" />
              </FlowPane.margin>
            </Button>
          </children>
        </FlowPane>
        <TableView id="tb_Tablea" fx:id="tb_Tabela" onMouseClicked="#evt_click" prefHeight="223.0" prefWidth="516.0">
          <columns>
            <TableColumn prefWidth="75.0" text="Código" fx:id="col_codigo" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="439.0" text="Nome" fx:id="col_nome" />
          </columns>
        </TableView>
      </children>
    </VBox>
  </items>
</SplitPane>
